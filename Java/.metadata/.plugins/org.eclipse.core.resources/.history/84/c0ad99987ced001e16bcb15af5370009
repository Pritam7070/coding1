package pkg1.cric;
import java.util.ArrayList;

public class Test1 {

	public static void main(String[] args) {
		String[] arr1= {"cat","rat","bat","cat","rat","cat"};
		ArrayList<String> list1=new ArrayList<>();
		for(int i=0;i<6;i++) {
			list1.add(arr1[i]);
		}
		System.out.println(list1);

	}

}
